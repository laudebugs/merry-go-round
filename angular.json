{
  "version": 1,
  "cli": {
    "defaultCollection": "@nrwl/angular"
  },
  "defaultProject": "merry-go-round",

  "projects": {
    "merry-go-round": {
      "projectType": "application",
      "schematics": {
        "@schematics/angular:component": {
          "style": "scss"
        },
        "@schematics/angular:application": {
          "strict": true
        }
      },
      "root": "apps/merry-go-round",
      "sourceRoot": "apps/merry-go-round/src",
      "prefix": "merry-go-round",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/apps/merry-go-round",
            "index": "apps/merry-go-round/src/index.html",
            "main": "apps/merry-go-round/src/main.ts",
            "polyfills": "apps/merry-go-round/src/polyfills.ts",
            "tsConfig": "apps/merry-go-round/tsconfig.app.json",
            "aot": true,
            "assets": [
              "apps/merry-go-round/src/favicon.ico",
              "apps/merry-go-round/src/assets"
            ],
            "styles": [
              "./node_modules/@angular/material/prebuilt-themes/indigo-pink.css",
              "apps/merry-go-round/src/styles.scss"
            ],
            "scripts": []
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "apps/merry-go-round/src/environments/environment.ts",
                  "with": "apps/merry-go-round/src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "namedChunks": false,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "1mb",
                  "maximumError": "5mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "2kb",
                  "maximumError": "4kb"
                }
              ]
            }
          }
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "merry-go-round:build"
          },
          "configurations": {
            "production": {
              "browserTarget": "merry-go-round:build:production"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "merry-go-round:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "apps/merry-go-round/src/test.ts",
            "polyfills": "apps/merry-go-round/src/polyfills.ts",
            "tsConfig": "apps/merry-go-round/tsconfig.spec.json",
            "karmaConfig": "apps/merry-go-round/karma.conf.js",
            "assets": [
              "apps/merry-go-round/src/favicon.ico",
              "apps/merry-go-round/src/assets"
            ],
            "styles": [
              "./node_modules/@angular/material/prebuilt-themes/indigo-pink.css",
              "apps/merry-go-round/src/styles.scss"
            ],
            "scripts": []
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "apps/merry-go-round/tsconfig.app.json",
              "apps/merry-go-round/tsconfig.spec.json"
            ],
            "exclude": ["**/node_modules/**"]
          }
        },
        "e2e": {
          "builder": "@angular-devkit/build-angular:protractor",
          "options": {
            "protractorConfig": "e2e/protractor.conf.js",
            "devServerTarget": "merry-go-round:serve"
          },
          "configurations": {
            "production": {
              "devServerTarget": "merry-go-round:serve:production"
            }
          }
        }
      }
    },
    "merry-go-round-e2e": {
      "root": "apps/merry-go-round-e2e",
      "projectType": "application",
      "architect": {
        "e2e": {
          "builder": "@angular-devkit/build-angular:protractor",
          "options": {
            "protractorConfig": "apps/merry-go-round-e2e/protractor.conf.js",
            "devServerTarget": "merry-go-round:serve"
          },
          "configurations": {
            "production": {
              "devServerTarget": "merry-go-round:serve:production"
            }
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": "apps/merry-go-round-e2e/tsconfig.json",
            "exclude": ["**/node_modules/**"]
          }
        }
      }
    },
    "core-data": {
      "projectType": "library",
      "root": "libs/core-data",
      "sourceRoot": "libs/core-data/src",
      "prefix": "merry-go-round",
      "architect": {
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "libs/core-data/src/**/*.ts",
              "libs/core-data/src/**/*.html"
            ]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "outputs": ["coverage/libs/core-data"],
          "options": {
            "jestConfig": "libs/core-data/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    },
    "core-state": {
      "projectType": "library",
      "root": "libs/core-state",
      "sourceRoot": "libs/core-state/src",
      "prefix": "merry-go-round",
      "architect": {
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "libs/core-state/src/**/*.ts",
              "libs/core-state/src/**/*.html"
            ]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "outputs": ["coverage/libs/core-state"],
          "options": {
            "jestConfig": "libs/core-state/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    },
    "material": {
      "projectType": "library",
      "root": "libs/material",
      "sourceRoot": "libs/material/src",
      "prefix": "merry-go-round",
      "architect": {
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "libs/material/src/**/*.ts",
              "libs/material/src/**/*.html"
            ]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "outputs": ["coverage/libs/material"],
          "options": {
            "jestConfig": "libs/material/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    }
  },
  "schematics": {
    "@nrwl/angular:application": {
      "style": "scss",
      "linter": "eslint",
      "unitTestRunner": "jest"
    },
    "@nrwl/angular:library": {
      "style": "scss",
      "linter": "eslint",
      "unitTestRunner": "jest"
    },
    "@nrwl/angular:component": {
      "style": "scss"
    }
  }
}
